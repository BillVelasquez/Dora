<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui">
	<ribbon startFromScratch="false">
		<tabs>
			<tab id="customTab" label="Dora">
				<group id="doraGrpDocumento" label="Documento">
					<button id="doraBtnNumerar" label="Numerar" size="large" onAction="doraNumerar" imageMso="FileNew" />
					<button id="doraBtnGuardar" label="Guardar" size="large" onAction="doraGuardar" imageMso="FileSave" />
					<button id="doraBtnSeleccionar" label="Seleccionar" size="large" onAction="doraSeleccionar" imageMso="SelectEntities" />
				</group>
				<group id="doraGrpInformes" label="Informes">
					<button id="doraBtnInformes" label="Informes" size="large" onAction="doraInformes" imageMso="ReportsProject" />
					<button id="doraBtnActualizar" label="Actualizar" size="large" onAction="doraInformes" imageMso="RefreshAll" />
					<button id="doraBtnConfiguracion" label="Configuracion"  onAction="doraConfiguracion" imageMso="PropertiesForm" />
				</group>
			</tab>
		</tabs>
	</ribbon>
</customUI>